using "Drawing.Color"using "Drawing.Bitmap"using "UI.Gadget"using "UI.Calendar"using "UI.Counter"using "UI.ColorPicker"using "UI.ImageGadget"using "UI.Button"using "UI.Select"using "UI.CheckBox"using "UI.Label"using "UI.Radio"using "UI.RadioGroup"using "UI.ScrollArea"using "UI.ProgressBar"using "UI.TextBox"using "UI.TextArea"using "UI.Window"local wnd1local wnd2local wnd3local cntrlocal clrpckrlocal clndrlocal imggadgetlocal btnlocal slclocal chkbxlocal lbllocal lbl1local lbl2local lbl3local radgrplocal rad1local rad2local rad3local scrllarealocal prglocal txtbxlocal txtareafunction main()	-- Radio box test	wnd1 = Window( 80 , 47 , 5 , 5 , "Radiobuttons" , "notResizeable" )		radgrp = RadioGroup()	rad1 = Radio( 3 , 3 , radgrp )	rad2 = Radio( 3 , 13 , radgrp )	rad3 = Radio( 3 , 23 , radgrp )	lbl1 = Label( 15 , 4 , "Option 1" )	lbl2 = Label( 15 , 14 , "Option 2" )	lbl3 = Label( 15 , 24 , "Option ...3!" )		wnd1.base.addChild( rad1 )	wnd1.base.addChild( rad2 )	wnd1.base.addChild( rad3 )		wnd1.base.addChild( lbl1 )	wnd1.base.addChild( lbl2 )	wnd1.base.addChild( lbl3 )		System.addChild( wnd1 )	--		-- Colorpicker, calendar Test	wnd2 = Window( 168 , 100 , 5 , 5 , "Special Gadgets" , "notResizeable" )		clrpckr = ColorPicker( 60 , 40 , 3 , 3 )	clrpckr.base.setInternalEventHandler( "onEdit" , colorHandler )	clndr = Calendar( 99 , 80 , 66 , 3 , 2013 , 6 , 5 )		wnd2.base.addChild( clrpckr )	wnd2.base.addChild( clndr )	--		System.addChild( wnd2 )endfunction colorHandler( event )	local bmp = Bitmap( 6 , 6 )	local col = Color( clrpckr.color )		if col.lum < 50 then		bmp.reset( System.rgb( 31 , 31 , 31 ) )	else		bmp.reset( System.rgb( 0 , 0 , 0 ) )	end	bmp.drawFilledRect( 1 , 1 , 4 , 4 , clrpckr.color )	wnd2.icon = bmpend